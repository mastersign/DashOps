  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\DashOps\DashOps.csproj" />
  </ItemGroup>
  <ItemGroup>
    <BindPath Include="..\..\publish\$(Platform)">
      <BindName>bin</BindName>
    </BindPath>
  </ItemGroup>
</Project>